{

  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "no-restricted-syntax": [
      "error",
      "ForStatement"
    ],
    "indent": [
      "error",
      2, {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single",
      "avoid-escape"
    ],
    "semi": [
      "error",
      "never",
      { "beforeStatementContinuationChars": "always" }
    ],
    "strict": [
      "error",
      "global"
    ],
    "curly": [
      "error"
    ],
    "no-cond-assign": [
      "error",
      "always"
    ],
    "no-dupe-keys": [
      "error"
    ],
    "no-duplicate-case": [
      "error"
    ],
    "no-sparse-arrays": [
      "warn"
    ],
    "no-obj-calls": [
      "error"
    ],
    "no-unexpected-multiline": [
      "error"
    ],
    "valid-typeof": [
      "error"
    ],
    "no-irregular-whitespace": [
      "error"
    ],
    "vars-on-top": [
      "warn"
    ],
    "space-before-function-paren": [
      "error"
    ],
    "space-before-blocks": [
      "error"
    ],
    "space-in-parens": [
      "error",
      "never"
    ],
    "space-infix-ops": [
      "error"
    ],
    "spaced-comment": [
      "error",
      "always"
    ],
    "space-unary-ops": [
      "error"
    ],
    "keyword-spacing": [
      "error"
    ],
    "guard-for-in": [
      "warn"
    ],
    "require-await": [
      "warn"
    ],
    "require-yield": [
      "warn"
    ],
    "no-return-await": [
      "warn"
    ],
    "no-await-in-loop": [
      "warn"
    ],
    "default-case": [
      "error"
    ],
    "eqeqeq": [
      "warn"
    ],
    "no-eq-null": [
      "warn"
    ],
    "no-eval": [
      "error"
    ],
    "no-empty-function": [
      "warn"
    ],
    "no-extra-bind": [
      "warn"
    ],
    "no-implied-eval": [
      "error"
    ],
    "complexity": [
      "warn",
      {
        "max": 15
      }
    ],
    "no-implicit-coercion": [
      "warn"
    ],
    "no-implicit-globals": [
      "warn"
    ],
    "no-invalid-this": [
      "error"
    ],
    "no-lone-blocks": [
      "warn"
    ],
    "no-loop-func": [
      "warn"
    ],
    "no-magic-numbers": [
      "warn",
      {
        "ignore": [0, 1, 2, 60, 1000],
        "enforceConst": true
      }
    ],
    "no-return-assign": [
      "warn"
    ],
    "no-self-compare": [
      "warn"
    ],
    "no-unused-expressions": [
      "warn"
    ],
    "no-useless-concat": [
      "warn"
    ],
    "no-useless-return": [
      "warn"
    ],
    "no-with": [
      "error"
    ],
    "yoda": [
      "warn"
    ],
    "no-plusplus": [
      "error"
    ],
    "prefer-destructuring": [
      "error",
      {
        "array": true,
        "object": true
      }
    ],
    "no-warning-comments": [
      "error",
      {
        "terms": ["todo", "fixme"]
      }
    ],
    "require-jsdoc": [
      "error"
    ],
    "id-length": [
      "error",
      {
        "min": 2,
        "max": 35
      }
    ],
    "getter-return": [
      "error"
    ],
    "no-useless-escape": [
      "off"
    ],
    "no-template-curly-in-string": [
      "warn"
    ],
    "require-atomic-updates": [
      "error"
    ],
    "accessor-pairs": [
      "error"
    ],
    "array-callback-return": [
      "warn"
    ],
    "consistent-return": [
      "warn"
    ],
    "no-caller": [
      "error"
    ],
    "no-extend-native": [
      "error"
    ],
    "no-iterator": [
      "error"
    ],
    "no-labels": [
      "error"
    ],
    "no-multi-str": [
      "warn"
    ],
    "no-new-wrappers": [
      "error"
    ],
    "no-new-func": [
      "error"
    ],
    "no-new": [
      "error"
    ],
    "no-param-reassign": [
      "error"
    ],
    "no-proto": [
      "warn"
    ],
    "no-sequences": [
      "warn"
    ],
    "no-useless-call": [
      "warn"
    ],
    "no-void": [
      "warn"
    ],
    "prefer-promise-reject-errors": [
      "warn"
    ],
    "radix": [
      "error"
    ],
    "require-unicode-regexp": [
      "warn"
    ],
    "no-shadow": [
      "error"
    ],
    "no-shadow-restricted-names": [
      "error"
    ],
    "no-undefined": [
      "error"
    ],
    "global-require": [
      "error"
    ],
    "handle-callback-err": [
      "warn"
    ],
    "no-buffer-constructor": [
      "error"
    ],
    "no-mixed-requires": [
      "warn"
    ],
    "no-new-require": [
      "error"
    ],
    "no-path-concat": [
      "error"
    ],
    "no-sync": [
      "error"
    ],
    "block-spacing": [
      "warn",
      "always"
    ],
    "brace-style": [
      "warn",
      "1tbs"
    ],
    "camelcase": [
      "warn",
      {
        "properties": "always",
        "ignoreDestructuring": false
      }
    ],
    "comma-dangle": [
      "warn",
      "never"
    ],
    "comma-spacing": [
      "warn",
      {
        "before": false,
        "after": true
      }
    ],
    "computed-property-spacing": [
      "warn",
      "never"
    ],
    "consistent-this": [
      "error",
      "self"
    ],
    "eol-last": [
      "warn",
      "always"
    ],
    "func-call-spacing": [
      "warn",
      "never"
    ],
    "function-paren-newline": [
      "warn",
      {
        "minItems": 5
      }
    ],
    "lines-between-class-members": [
      "warn",
      "always",
      { "exceptAfterSingleLine": true }
    ],
    "max-depth": [
      "warn",
      5
    ],
    "max-len": [
      "warn",
      120
    ],
    "max-lines": [
      "warn",
      500
    ],
    "max-lines-per-function": [
      "warn",
      100
    ],
    "max-nested-callbacks": [
      "warn",
      5
    ],
    "max-params": [
      "warn",
      10
    ],
    "max-statements": [
      "warn",
      30
    ],
    "max-statements-per-line": [
      "warn",
      {
        "max": 1
      }
    ],
    "multiline-ternary": [
      "warn",
      "never"
    ],
    "new-cap": [
      "error",
      {
        "capIsNewExceptions": [
          "GET",
          "POST",
          "PUT",
          "DELETE"
        ]
      }
    ],
    "new-parens": [
      "warn"
    ],
    "newline-per-chained-call": [
      "warn",
      {
        "ignoreChainWithDepth": 3
      }
    ],
    "no-array-constructor": [
      "warn"
    ],
    "no-bitwise": [
      "warn"
    ],
    "no-lonely-if": [
      "warn"
    ],
    "no-mixed-operators": [
      "warn"
    ],
    "no-multi-assign": [
      "warn"
    ],
    "no-negated-condition": [
      "warn"
    ],
    "no-nested-ternary": [
      "warn"
    ],
    "no-new-object": [
      "warn"
    ],
    "no-underscore-dangle": [
      "warn",
      {
        "allow": [
          "_readableState"
        ]
      }
    ],
    "no-unneeded-ternary": [
      "warn"
    ],
    "no-whitespace-before-property": [
      "warn"
    ],
    "operator-linebreak": [
      "warn",
      "before"
    ],
    "prefer-object-spread": [
      "warn"
    ],
    "arrow-body-style": [
      "error",
      "always"
    ],
    "arrow-parens": [
      "error",
      "always"
    ],
    "arrow-spacing": [
      "warn",
      {
        "before": true,
        "after": true
      }
    ],
    "generator-star-spacing": [
      "warn",
      "both"
    ],
    "no-confusing-arrow": [
      "error"
    ],
    "no-duplicate-imports": [
      "warn"
    ],
    "no-useless-constructor": [
      "warn"
    ],
    "no-useless-computed-key": [
      "warn"
    ],
    "no-useless-rename": [
      "warn"
    ],
    "object-shorthand": [
      "warn"
    ],
    "prefer-arrow-callback": [
      "error"
    ],
    "prefer-const": [
      "warn"
    ],
    "prefer-numeric-literals": [
      "warn"
    ],
    "prefer-rest-params": [
      "warn"
    ],
    "prefer-spread": [
      "warn"
    ],
    "prefer-template": [
      "warn"
    ],
    "rest-spread-spacing": [
      "warn",
      "never"
    ],
    "symbol-description": [
      "warn"
    ],
    "template-curly-spacing": [
      "warn",
      "never"
    ],
    "yield-star-spacing": [
      "warn",
      "both"
    ],
    "object-curly-spacing": [
      "warn",
      "always"
    ],
    "import/first": [
      "error"
    ],
    "import/newline-after-import": [
      "warn"
    ],
    "import/prefer-default-export": [
      "error"
    ],
    "import/no-unassigned-import": [
      "error"
    ],
    "import/no-anonymous-default-export": [
      "warn"
    ]
  }
}
